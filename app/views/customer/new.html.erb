<h2>登録</h2>


<%= labelled_form_for @customer, :as => :customer,:url => "" do |form| %>

<%= error_messages_for 'customer' %>
<div class="box">
<p><%= form.text_field :family_name,
                       :size => 20, :required => true %></p>
<p><%= form.text_field :first_name,
                       :size => 20, :required => true %></p>
<p><%= form.text_field :address,
                       :size => 20, :required => true %></p>
<p><%= form.text_field :phone_number,
                       :size => 20, :required => true %></p>
</div>
<input hidden="project_id" value="<%= @project.identifier %>" />
                       

    <%= form.submit l(:button_create) %>
<% end %>