<h2>GeoInfoController#list</h2>


<table>

  <tr>
    <td>ID</td><td>l("label_name")</td><td><%= l("label_longitude") %></td><td><%= l("label_latitude") %></td>
  </tr>  
  <% for g in geo_infos %>
  
  <tr>
    <td><%= g.id %></td><td><%= g.name %></td><td><%= g.longitude %></td><td><%= g.latitude %></td>
  </tr>  
  <% end %>
</table>